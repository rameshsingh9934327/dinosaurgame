    <script>
        // --- 2D Game Logic ---
        const dino=document.getElementById("dino"),obstacle=document.getElementById("obstacle"),scoreElement=document.getElementById("score"),hiScoreElement=document.getElementById("hiscore"),gameOverMessage=document.getElementById("game-over-message"),gameOverTitle=document.getElementById("game-over-title"),finalScoreText=document.getElementById("final-score-text"),restartButton=document.getElementById("restart-button"),gameContainer=document.getElementById("game-container"),pauseIconButton=document.getElementById("pause-icon-button"),muteIconButton=document.getElementById("mute-icon-button"),autoplayButton=document.getElementById("autoplay-button"),exitButton=document.getElementById("exit-button"),root=document.documentElement;
        const RUNNING_DINO_SRC="assets/dinogame/running-dinosaur.gif",COLLISION_DINO_SRC="assets/dinogame/dinosaur-obstacle.png";
        const PAUSE_ICON_SVG='<svg viewBox="0 0 24 24"><path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/></svg>',RESUME_ICON_SVG='<svg viewBox="0 0 24 24"><path d="M8 5v14l11-7z"/></svg>',MUTE_ICON_SVG='<svg viewBox="0 0 24 24"><path d="M16.5 12c0-1.77-1.02-3.29-2.5-4.03v2.21l2.45 2.45c.03-.2.05-.41.05-.63zm2.5 0c0 .94-.2 1.82-.54 2.64l1.51 1.51C20.63 14.91 21 13.5 21 12c0-4.28-2.99-7.86-7-8.77v2.06c2.89.86 5 3.54 5 6.71zM4.27 3L3 4.27 7.73 9H3v6h4l5 5v-6.73l4.25 4.25c-.67.52-1.42.93-2.25 1.18v2.06c1.38-.31 2.63-.95 3.69-1.81L19.73 21 21 19.73l-9-9L4.27 3zM12 4L9.91 6.09 12 8.18V4z"/></svg>',UNMUTE_ICON_SVG='<svg viewBox="0 0 24 24"><path d="M3 9v6h4l5 5V4L7 9H3zm13.5 3c0-1.77-1.02-3.29-2.5-4.03v8.05c1.48-.73 2.5-2.25 2.5-4.02zM14 3.23v2.06c2.89.86 5 3.54 5 6.71s-2.11 5.85-5 6.71v2.06c4.01-.91 7-4.49 7-8.77s-2.99-7.86-7-8.77z"/></svg>';
        
        const jumpSound=new Audio("assets/audio/jump.mp3"),gameOverSound=new Audio("assets/audio/gameover.mp3");
        let score=0,hiScore=localStorage.getItem("dinoHiScore2D")||0,isGameOver=!0,isPaused=!1,isMuted="true"===localStorage.getItem("dinoGameMuted2D"),isAutoPlay="true"===localStorage.getItem("dinoAutoPlay2D"),lastTime=0,currentSpeed,scoreCounter=0;const cactusImages=["assets/dinogame/cactus1.png","assets/dinogame/cactus2.png"],INITIAL_SPEED=280,SPEED_INCREMENT=.2;
        const themes={
            light:  { bg: "#f7f7f7", text: "#333", ground: "#333", obs: "#d9534f", cloud: "#ddebf0", filter: "none", pageBg: "#ffffff", pageText: "#2c3e50" },
            dark:   { bg: "#2c3e50", text: "#ecf0f1", ground: "#ecf0f1", obs: "#e74c3c", cloud: "#34495e", filter: "brightness(0.9) grayscale(0.2)", pageBg: "#121212", pageText: "#e0e0e0" },
            gorilla:{ bg: "#2a403d", text: "#a3ffb4", ground: "#32cd32", obs: "#32cd32", cloud: "#415a56", filter: "hue-rotate(220deg) saturate(2)", pageBg: "#2b0033", pageText: "#ffffff" },
            yellow: { bg: "#fffacd", text: "#5c4033", ground: "#5c4033", obs: "#4682b4", cloud: "#ffffff", filter: "sepia(0.5) saturate(2)", pageBg: "#fff8b5", pageText: "#333300" },
            green:  { bg: "#e8f5e9", text: "#1b5e20", ground: "#2e7d32", obs: "#a5d6a7", cloud: "#c8e6c9", filter: "hue-rotate(30deg) saturate(1.5)", pageBg: "#ccffcc", pageText: "#003300" }
        };
        function handleAction(){isGameOver?startGame():jump()}
        function gameLoop(e){if(isGameOver||isPaused)return;isAutoPlay&&autoPlayLogic();const t=e-lastTime||0;moveObstacle(t),checkCollision(),updateScore(t),lastTime=e,requestAnimationFrame(gameLoop)}
        function startGame(){isGameOver=!1,isPaused=!1,score=0,scoreCounter=0,currentSpeed=INITIAL_SPEED,dino.classList.remove("collided"),scoreElement.innerText="00000",dino.src=RUNNING_DINO_SRC,gameOverMessage.classList.add("hidden"),pauseIconButton.style.display="block",muteIconButton.style.display="block",resetObstacle(),obstacle.style.visibility="hidden",setTimeout(()=>{isGameOver||(obstacle.style.visibility="visible")},500),document.querySelectorAll(".cloud").forEach(e=>{e.style.animationPlayState="running"}),lastTime=performance.now(),requestAnimationFrame(gameLoop)}
        function endGame() {
            isGameOver = !0;
            const finalScore = Math.floor(score);
            const submitButton = document.getElementById("submit-score-button");
            submitButton.href = `leaderboard.html?score=${finalScore}`;

            isMuted || (gameOverSound.currentTime = 0, gameOverSound.play()), dino.src = COLLISION_DINO_SRC, dino.classList.add("collided"), pauseIconButton.style.display = "none", muteIconButton.style.display = "none", gameContainer.classList.add("game-over-shake"), setTimeout(() => gameContainer.classList.remove("game-over-shake"), 300), score > hiScore && (hiScore = Math.floor(score), localStorage.setItem("dinoHiScore2D", hiScore), hiScoreElement.innerText = String(hiScore).padStart(5, "0")), gameOverTitle.textContent = "GAME OVER", finalScoreText.textContent = `Your Score: ${finalScore}`, gameOverMessage.classList.remove("hidden"), document.querySelectorAll(".cloud").forEach(e => {
                e.style.animationPlayState = "paused"
            })
        }
        function jump(){if(isGameOver||dino.classList.contains("jump-animation")||isPaused)return;isMuted||(jumpSound.currentTime=0,jumpSound.play()),dino.classList.add("jump-animation"),dino.addEventListener("animationend",()=>dino.classList.remove("jump-animation"),{once:!0})}
        function autoPlayLogic(){if(dino.classList.contains("jump-animation"))return;const e=obstacle.getBoundingClientRect(),t=dino.getBoundingClientRect();if(e.right<0||e.left>gameContainer.clientWidth)return;const o=currentSpeed*.38+30;e.left>t.left&&e.left<t.left+o&&jump()}
        function checkCollision(){if(isGameOver||isAutoPlay)return;const e=dino.getBoundingClientRect(),t=obstacle.getBoundingClientRect();"hidden"!==getComputedStyle(obstacle).visibility&&e.right-25>t.left&&e.left+25<t.right&&e.bottom>t.top+15&&endGame()}
        function moveObstacle(e){if(isPaused||"hidden"===getComputedStyle(obstacle).visibility)return;let t=parseFloat(getComputedStyle(obstacle).left);t-=currentSpeed*e/1e3,t<-100&&(t=gameContainer.clientWidth,obstacle.src=cactusImages[Math.floor(2*Math.random())]),obstacle.style.left=t+"px"}
        function updateScore(e){scoreCounter+=e,scoreCounter>=100&&(score++,scoreElement.innerText=String(Math.floor(score)).padStart(5,"0"),currentSpeed+=SPEED_INCREMENT,scoreCounter=0)}
        function resetObstacle(){obstacle.style.left=gameContainer.clientWidth+"px",obstacle.src=cactusImages[Math.floor(2*Math.random())]}
        function togglePause(){isGameOver||(isPaused=!isPaused,pauseIconButton.innerHTML=isPaused?RESUME_ICON_SVG:PAUSE_ICON_SVG,document.querySelectorAll(".cloud").forEach(e=>{e.style.animationPlayState=isPaused?"paused":"running"}),isPaused||(lastTime=performance.now(),requestAnimationFrame(gameLoop)))}
        function toggleMute(){isMuted=!isMuted;localStorage.setItem("dinoGameMuted2D",isMuted);muteIconButton.innerHTML=isMuted?MUTE_ICON_SVG:UNMUTE_ICON_SVG}
        function toggleAutoPlay(){isAutoPlay=!isAutoPlay;localStorage.setItem("dinoAutoPlay2D",isAutoPlay);updateAutoPlayButton()}
        function updateAutoPlayButton(){autoplayButton.textContent=`Auto Play: ${isAutoPlay?"ON":"OFF"}`,autoplayButton.classList.toggle("active",isAutoPlay)}
        
        function applyTheme(themeName) {
            const theme = themes[themeName] || themes.light;
            root.style.setProperty("--page-bg-color", theme.pageBg);
            root.style.setProperty("--page-text-color", theme.pageText);
            root.style.setProperty("--bg-color", theme.bg);
            root.style.setProperty("--primary-text-color", theme.text);
            root.style.setProperty("--ground-color", theme.ground);
            root.style.setProperty("--obstacle-color", theme.obs);
            root.style.setProperty("--cloud-color", theme.cloud);
            root.style.setProperty("--dino-filter", theme.filter);
            localStorage.setItem("dino_game_theme_2d", themeName);
            document.getElementById('themeSelectorDesktop').value = themeName;
            document.getElementById('themeSelectorMobile').value = themeName;

            document.querySelectorAll('#theme-switcher-game button').forEach(button => {
                button.classList.toggle('active-theme', button.dataset.theme === themeName);
            });
        }
        
        function initializeGame(){hiScoreElement.innerText=String(Math.floor(hiScore)).padStart(5,"0"),finalScoreText.textContent="Press Space or Tap to Start",gameOverMessage.classList.remove("hidden"),dino.src=COLLISION_DINO_SRC,dino.classList.add("collided"),pauseIconButton.innerHTML=PAUSE_ICON_SVG,muteIconButton.innerHTML=isMuted?MUTE_ICON_SVG:UNMUTE_ICON_SVG,pauseIconButton.style.display="none",muteIconButton.style.display="none",updateAutoPlayButton(),resetObstacle(),obstacle.style.visibility="hidden",gameOverTitle.textContent="DINO GAME"}
        
        // --- UI & Fullscreen Logic ---
        function toggleMenu(){document.getElementById('mobileMenu').classList.toggle('open')}
        function openFullscreen(elem){if(elem.requestFullscreen){elem.requestFullscreen()}else if(elem.webkitRequestFullscreen){elem.webkitRequestFullscreen()}}
        function exitGame() {
            if (document.exitFullscreen) {
                document.exitFullscreen();
            } else if (document.webkitExitFullscreen) { /* Safari */
                document.webkitExitFullscreen();
            }
        }
        document.addEventListener('DOMContentLoaded',()=>{
            const gameScreenWrapper=document.getElementById('game-screen-wrapper');
            document.getElementById('playNowButton').addEventListener('click',()=>{gameScreenWrapper.style.display='flex';openFullscreen(gameScreenWrapper);startGame()});
            document.addEventListener('fullscreenchange',()=>{if(!document.fullscreenElement){gameScreenWrapper.style.display='none';isGameOver=!0}});
            document.addEventListener("keydown",e=>{if(document.fullscreenElement){e.preventDefault();"Space"===e.code||"ArrowUp"===e.code?handleAction():"KeyP"===e.code?togglePause():"KeyM"===e.code&&toggleMute()}});
            
            document.addEventListener("touchstart",e=>{
                if(!document.fullscreenElement)return;
                const t=e.target;
                if(t.closest("#instructions-container")||t.closest("#pause-icon-button")||t.closest("#mute-icon-button") || t.closest("#theme-switcher-game"))return;
                e.preventDefault();
                handleAction();
            });

            gameOverMessage.addEventListener('touchstart',e=>e.stopPropagation());
            restartButton.addEventListener("click",startGame);
            pauseIconButton.addEventListener("click",togglePause);
            muteIconButton.addEventListener("click",toggleMute);
            autoplayButton.addEventListener("click",toggleAutoPlay);
            exitButton.addEventListener("click", exitGame);
            
            // Setup for all theme selectors
            ['themeSelectorDesktop','themeSelectorMobile'].forEach(id=>document.getElementById(id).addEventListener('change',function(){applyTheme(this.value)}));
            document.querySelectorAll('#theme-switcher-game button').forEach(button => {
                button.addEventListener('click', function() {
                    applyTheme(this.dataset.theme);
                });
            });

            const savedTheme=localStorage.getItem("dino_game_theme_2d")||"light";applyTheme(savedTheme);
            initializeGame();

            // FAQ Accordion Logic
            document.querySelectorAll('.faq-question').forEach(question => {
                question.addEventListener('click', () => {
                    const faqItem = question.parentElement;
                    faqItem.classList.toggle('active');
                });
            });
        });
    </script>
</body>
</html>
